{
  "expo": {
    "name": "Goods Info API",
    "slug": "goods-info-api",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.ganeshghate.goodsinfoapi",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "INTERNET",
        "SYSTEM_ALERT_WINDOW",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "DOWNLOAD_WITHOUT_NOTIFICATION"
      ],
      "intentFilters": [
        {
          "autoVerify": true,
          "action": ["MAIN", "DOWNLOAD_COMPLETE"],
          "data": {
            "scheme": "https",
            "host": "*.example.com"
          },
          "category": ["LAUNCHER"]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ganeshghate.goodsinfoapi"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splash-icon-main.png",
          "dark": {
            "image": "./assets/images/splash-icon-main.png",
            "backgroundColor": "#ffffff"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Work_Sans/static/WorkSans-Regular.ttf"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/splash-icon-main.png",
      "backgroundColor": "#ffffff",
      "imageWidth": 100
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6d94246f-4515-4fdb-b0de-0b0fd4a7bd6b"
      }
    }
  }
}
